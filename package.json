{
  "name": "@dringtech/openapi-stubber",
  "version": "0.7.0",
  "description": "Auto-stubbing for OpenAPI V3 APIs",
  "license": "ISC",
  "main": "lib",
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "test",
    "stub",
    "mock",
    "openapi"
  ],
  "scripts": {
    "test": "mocha",
    "build": "npm prune",
    "lint": "eslint .",
    "precoverage": "nyc --all npm test",
    "coverage": "nyc check-coverage",
    "mutation": "stryker run",
    "scan": "npm audit"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^2.0.0",
    "@stryker-mutator/html-reporter": "^2.0.0",
    "@stryker-mutator/javascript-mutator": "^2.0.0",
    "@stryker-mutator/mocha-framework": "^2.0.0",
    "@stryker-mutator/mocha-runner": "^2.0.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-fs": "^2.0.0",
    "clear-module": "^3.2.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "mocha": "^10.2.0",
    "mocha-multi-reporters": "^1.1.7",
    "mochawesome": "^4.0.0",
    "nyc": "^14.1.1",
    "proxyquire": "^2.1.0",
    "rewire": "^4.0.1",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0"
  },
  "dependencies": {
    "express": "^4.17.1",
    "express-winston": "^3.1.0",
    "openapi-backend": "^2.3.2",
    "winston": "^3.2.1"
  }
}
